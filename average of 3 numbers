// WRITE A PROGRAM TO FIND THE AVERAGE OF 3 NUMBERS 
#include<stdio.h>
int main(){
    int a,b,c;
    printf("ENTER FIRST NUMBER : ");
    scanf("%d",&a);
    printf("ENTER SECOND NUMBER : ");
    scanf("%d",&b);
    printf("ENTER THIRD NUMBER : ");
    scanf("%d",&c);
    printf("THE AVERAGE IS %d ",(a+b+c)/3);
    return 0;
}