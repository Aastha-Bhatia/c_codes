// write a program in c to find the maximum number between two numbers using a pointer
#include<stdio.h>
void max(int a,int b);
int main(){
    int a,b;
    printf("Enter a:");
    scanf("%d",&a);
    printf("Enter b:");
    scanf("%d",&b);
    max(a,b);
    return 0;
}
void max(int a,int b){
    if(a>b){
        printf("a is greater");
    }
    else{
        printf("b is greater");
    }
}
